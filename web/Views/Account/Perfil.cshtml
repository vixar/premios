                                type: 'warning',
                                showCancelButton: false,
                                confirmButtonColor: '#3085d6',
                                cancelButtonColor: '#d33',
                                confirmButtonText: 'De acuerdo'
                            }).then((result) => {
                                if (result.value) {
                                    location.reload();
                                }

                            @*@Html.ValidationMessageFor(model => model.Cargo, "", new { @class = "text-danger" })*@
                        </div><!-- End .from-group -->
                    </div><!-- End .col-sm-6 -->
                    

                </div><!-- End .row -->

                <div class="row">

                    <div class="col-sm-6 ">

                        <div class="form-group">
                            <label>Número de Cédula</label> <i style="color: #ce8a8a" class="fa fa-asterisk"></i>

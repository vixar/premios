            value="Disabled" />-->

	</appSettings>
	<system.web>

		<authentication mode="None" />
		<compilation debug="true" targetFramework="4.5.2" />
		<httpRuntime targetFramework="4.5.2" />
		<!--<httpModules>
      <add name="ApplicationInsightsWebTracking" type="Microsoft.ApplicationInsights.Web.ApplicationInsightsHttpModule, Microsoft.AI.Web" />
    </httpModules>-->
		<!--<customErrors defaultRedirect="~/Home/ErrorPage" mode="On">
      <error statusCode="404" redirect="~/Home/ErrorPag"/>
      <error statusCode="500" redirect="~/Home/ErrorPag"/>
      <error statusCode="401" redirect="~/Home/ErrorPag"/>

    </customErrors>-->
		<httpModules>
			<add name="ApplicationInsightsWebTracking" type="Microsoft.ApplicationInsights.Web.ApplicationInsightsHttpModule, Microsoft.AI.Web" />
			<add name="TelemetryCorrelationHttpModule" type="Microsoft.AspNet.TelemetryCorrelation.TelemetryCorrelationHttpModule, Microsoft.AspNet.TelemetryCorrelation" />
		</httpModules>
	</system.web>
	<system.webServer>
		<!--<httpErrors errorMode="Custom">
      <remove statusCode="404"/>
      <error statusCode="404" path="~/Shared/ErrorPage" responseMode="ExecuteURL"/>
    </httpErrors>-->
		<modules runAllManagedModulesForAllRequests="true">
			<remove name="FormsAuthentication" />
			<remove name="ApplicationInsightsWebTracking" />
			<add name="ApplicationInsightsWebTracking" type="Microsoft.ApplicationInsights.Web.ApplicationInsightsHttpModule, Microsoft.AI.Web" preCondition="managedHandler" />
			<remove name="TelemetryCorrelationHttpModule" />
			<add name="TelemetryCorrelationHttpModule" type="Microsoft.AspNet.TelemetryCorrelation.TelemetryCorrelationHttpModule, Microsoft.AspNet.TelemetryCorrelation" preCondition="managedHandler" />
		</modules>
		<validation validateIntegratedModeConfiguration="false" />
	</system.webServer>
	<location path="signalr/hubs">
		<system.web>
			<authorization>
				<allow users="*" />
			</authorization>
		</system.web>
		<system.webServer>
			<security>
				<authorization>
					<!--add accessType="Allow" users="*"/-->
				</authorization>
			</security>
		</system.webServer>
	</location>

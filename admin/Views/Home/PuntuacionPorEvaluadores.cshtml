@using ProyectoCalidad.Domain.Entities
@model  IEnumerable<ProyectoCalidad.Domain.Entities.Premio>

@{

    ApplicationUser user = null;

    if (ViewBag.user != null)
    {
        user = ViewBag.user;
    }

    if (Model != null)
    {
        <ul class="list-unstyled">
            @foreach (var item in Model)
            {

                    <li>
                        @if (user != null)
                        {
                            <a href="@Url.Action("InstitucionesPostulantes", "Reportes", new {PremioID = item.PremioID, userID = user.Id})"><i class="mdi mdi-trophy"></i> @item.TipoPremio.Siglas</a>

                        }
                        else
                        {
                            <p class="text-danger">ID Usuario Inválido</p>

                        }
                    </li>




            }
        </ul>


    }

}



                            <ul class="list-unstyled">
                                <li><a href="@Url.Action("Index", "JuradoInstitucion", new { premioID = pre })">Asignados</a></li>

                                <li><a href="@Url.Action("SinAsignacion", "Jurado", new {premioID = pre})">No Asignados</a></li>
                            </ul>

                        }
                        break;
                    case "PPC":
                        foreach (var version in Model.ppc)
                        {
                            var ver = version.GetType().GetProperty("Version").GetValue(version, null);
                            var pre = version.GetType().GetProperty("PremioID").GetValue(version, null);
                            @*<li><a href="@Url.Action("Index", "InstitucionPostulante", new {premioID = version.PremioID})">Versión @Html.Raw(version.Version == null ? "2019" : version.Version.ToString())</a></li>*@

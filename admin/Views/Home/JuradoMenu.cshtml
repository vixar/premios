                            <ul class="list-unstyled">
                                <li><a href="@Url.Action("Index", "JuradoInstitucion", new { premioID = pre })">Asignados</a></li>

                                <li><a href="@Url.Action("SinAsignacion", "Jurado", new {premioID = pre})">No Asignados</a></li>
                            </ul>

                        }
                        break;
                    default:
                        break;
                }
            </ul>


            @*<ul class="list-unstyled">
                    <li><a href="@Url.Action("Index", "JuradoInstitucion", new { premioID = item.PremioID })">Asignados</a></li>

                    <li><a href="@Url.Action("SinAsignacion", "Jurado", new {premioID = item.PremioID})">No Asignados</a></li>

                </ul>

                <li>
                    <a href="javascript:void(0);" class="waves-effect"><i class="mdi mdi-trophy"></i><span>PNC</span> <span class="menu-arrow"></span></a>

                    <ul class="list-unstyled">
                        <a href="javascript:void(0);" class="waves-effect"><i class="mdi mdi-trophy"></i><span> Asignados </span> <span class="menu-arrow"></span></a>

                        <ul class="list-unstyled">
                            <li><a href="@Url.Action("Index", "JuradoInstitucion", new { premioID = 4 })">Versión 2020</a></li>

                            <li><a href="@Url.Action("Index", "JuradoInstitucion", new { premioID = 1 })">Versión 2019</a></li>

                        </ul>
                        <li><a href="@Url.Action("SinAsignacion", "Jurado", new {premioID = 1})">No Asignados</a></li>
                    </ul>


                </li>

                <a href="javascript:void(0);" class="waves-effect"><i class="mdi mdi-trophy"></i><span> PPC </span> <span class="menu-arrow"></span></a>

                <ul class="list-unstyled">
                    <li><a href="@Url.Action("Index", "JuradoInstitucion", new { premioID = 2 })">Asignados</a></li>

                    <li><a href="@Url.Action("SinAsignacion", "Jurado", new {premioID = 2})">No Asignados</a></li>


@model ProyectoCalidad.Domain.Entities.EquipoEvaluador


<div class="col-lg-12">
    <div class="panel panel-default">
        <!-- Default panel contents -->
        <div class="panel-heading">
            <h3 class="panel-title">@Model.Descripcion</h3>
        </div>
        @*<div class="panel-body">
                <p>
                </p>
            </div>*@

        <!-- List group -->
        <ul class="list-group">
            @if (Model.Estado)
            {
                <li class="list-group-item">

                    <b> Estado</b>: Activo

                </li>
            }
            else
            {
                <li class="list-group-item">

                    <b> Estado</b>: Inactivo

                </li>
            }
                <li class="list-group-item">
                    <b> Premio</b>: @Model.Premio.Descripcion
                </li>
            <li class="list-group-item">
                @{
                    ProyectoCalidad.Domain.Entities.Organismo Organismo = ViewBag.organismo;

                }
               
                <b> Institución</b>: @Organismo.Nombre
            </li>
                <li class="list-group-item">
                    <b> Fecha de Creación</b>: @Model.FechaHoraCreacion
                </li>
            <li class="list-group-item">
                Equipo ID: @Model.EquipoID
            </li>
        </ul>
    </div>
</div>


@model ProyectoCalidad.Domain.Entities.ApplicationUser

@{
    var mensaje = ViewBag.mensaje;
    List<SelectListItem> sector = new List<SelectListItem>();

    EvaluadorPremio evaluador = null;
    Jurado jurado = null;

    var userManager = new UserManager<ProyectoCalidad.Domain.Entities.ApplicationUser>(new UserStore<ProyectoCalidad.Domain.Entities.ApplicationUser>(new EFDbContext()));
    var user = userManager.FindById(User.Identity.GetUserId<string>());

    EquipoInfoViewModel datosEquipo = ViewBag.equipo;

    if (ViewBag.evaluador != null)
    {
        evaluador = ViewBag.evaluador;
    }

    if (ViewBag.jurado != null)
    {
        jurado = ViewBag.jurado;
    }


    sector.Add(new SelectListItem
    {
        Text = "Privado",
        Value = "Privado"
    });
    sector.Add(new SelectListItem
    {
        Text = "Público",
        Value = "Público"
    });

    <style>
        .crop {
            height: 100%;
            overflow: hidden;
            position: relative;
            display: block;
            min-width: 100%;
            min-height: 100%;
            margin: auto;
            position: absolute;
            top: -100%;
            right: -100%;
            bottom: -100%;
            left: -100%;
        }
    </style>
}



<div class="modal fade bs-cv-modal" tabindex="-1" role="dialog" aria-labelledby="myLargeModalLabel" aria-hidden="true" style="display: none;">
    <div class="modal-dialog modal-lg">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-hidden="true">×</button>
                <h4 class="modal-title" id="myLargeModalLabel">Currículum</h4>
            </div>
            <div class="modal-body">
                @{
                    var urlevidencia = Server.MapPath("~/Usuarios/Documentos/cv/");

                    var url = urlevidencia + Model.cv;

                    if (!string.IsNullOrEmpty(Model.cv))
                    {
                        if (System.IO.File.Exists(url))
                        {
                            <embed src="@Url.Content("/sgp_admin/Usuarios/Documentos/cv/" + Model.cv)" frameborder="0" width="100%" height="400px" />
                        }
                        else
                        {
                            <p>Este usuario no tiene cv cargado</p>
                        }

                    }
                    else
                    {
                        <p>Este usuario no tiene cv cargado</p>
                    }

                }
            </div>
        </div><!-- /.modal-content -->
    </div><!-- /.modal-dialog -->
</div><!-- /.modal -->

